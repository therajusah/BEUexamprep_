generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String?
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  role      String   @default("user")
}

model PdfUpload {
  id          String   @id @default(cuid())
  file_url    String
  branch      String
  semester    String
  subject     String
  uploaded_at DateTime @default(now())
}
